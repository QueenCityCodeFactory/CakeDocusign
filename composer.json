{
    "name": "queencitycodefactory/docusign",
    "description": "Docusign Utility plugin for CakePHP 3.x",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Growden",
            "email": "taylor@teamwilletts.com",
            "homepage": "https://github.com/tgrowden"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0",
        "docusign/esign-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Docusign\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docusign\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
